package projekt;

import java.util.ArrayList;

public class SzczegolnyDzienR {

	
	public String nazwa; 
	public String opis;
	public String praktyka;
	public ArrayList<RokDzienR> rokDzienR; //impl.asocjacji * z RokDzienR


public SzczegolnyDzienR(String nazwa, String opis, String praktyka){
	this.nazwa = nazwa;
	this.opis = opis;
	this.praktyka = praktyka;
	this.rokDzienR = new ArrayList<RokDzienR>();
		}

}

