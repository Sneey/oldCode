        for(int i=0;i<ciagPoczatkowy.length();i++){
        	lista.add(Character.getNumericValue((ciagPoczatkowy.indexOf(i))));
        }
        for(int i=0;i<10;i++){
        	//lista.add();
        }
        
        System.out.println(wielomian.charAt(2));
        System.out.println(ciagPoczatkowy.indexOf(1));
        

	}

