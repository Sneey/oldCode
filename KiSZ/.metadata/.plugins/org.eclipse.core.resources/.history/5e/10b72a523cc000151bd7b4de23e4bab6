package projekt;

public class JednorazoweW extends Wydarzenie{
	
	public float oplataD;
	public int ileDni; 
	//public float kosztC;
	
	public JednorazoweW(String nazwa, String opis, String harmonogram, float oplataD, int ileDni){
		super(nazwa, opis, harmonogram);
		this.oplataD = oplataD;
		this.ileDni = ileDni;
		}

	public float getKosztCalkowity(){
		return oplataD * ileDni;
	}
	
}
