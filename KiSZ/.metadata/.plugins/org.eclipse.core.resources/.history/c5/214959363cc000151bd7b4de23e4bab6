package projekt;

public class RokWyd {

	//atrybut przechowujacy rok,  zwiazane z wydarzeniem w danym dniu
		public int rok;
		public Wydarzenie wydarzenie;  //impl.licznosci 1 w celu implementacji asocjacji z atrybutem - wydarzenie
		public DanyDzien danyDzien; //impl.licznosci 1 w celu implementacji asocjacji z atrybutem
		
		public RokWyd(int rok, Wydarzenie wydarzenie, DanyDzien danyDzien){
			this.rok = rok; 
			this.wydarzenie = wydarzenie;
			this.danyDzien = danyDzien; 
		}
		

	}
